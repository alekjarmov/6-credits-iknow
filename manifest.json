{
  "manifest_version": 3,
  "name": "6 extra iKnow credits",
  "description": "This extension adds 6 extra iKnow credits",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.iknow.ukim.mk/*"],
      "js": ["content.js"]
    }
  ]
}